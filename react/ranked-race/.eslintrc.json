{
  "extends": ["next", "prettier"],
  "plugins": ["unused-imports"],
  "rules": {
    "no-unused-expressions": "warn",
    "unused-imports/no-unused-imports": "warn",
    "linebreak-style": "warn",
    "block-spacing": "warn",
    "no-trailing-spaces": "warn",
    "no-multiple-empty-lines": "warn"
  }
}
